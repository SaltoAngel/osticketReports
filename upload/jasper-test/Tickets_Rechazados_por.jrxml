	<queryString>
		<![CDATA[SELECT
      t.ticket_id   AS id,
      t.number      AS numero,
      DATE_FORMAT(t.created,'%Y-%m-%d %H:%i') AS fecha_creacion,
      st.name       AS estado,
      CONCAT(IFNULL(sf.firstname,''),' ',IFNULL(sf.lastname,'')) AS rechazado_por,
      tm.name       AS equipo,
      CONCAT(uc.name,' <',uc.email,'>') AS cliente
    FROM ost_ticket t
    LEFT JOIN ost_ticket_status st ON st.id = t.status_id
    LEFT JOIN ost_staff sf ON sf.staff_id = t.staff_id
    LEFT JOIN ost_team tm ON tm.team_id = t.team_id
    LEFT JOIN ost_user u ON u.id = t.user_id
    LEFT JOIN ost_user__cdata uc ON uc.user_id = u.id
    WHERE st.name IN ('Rejected','Rechazado')
    ORDER BY t.created DESC]]>
	</queryString>
