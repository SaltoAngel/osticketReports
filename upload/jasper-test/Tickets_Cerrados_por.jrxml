	<queryString>
		<![CDATA[SELECT
      t.ticket_id   AS id,
      t.number      AS numero,
      DATE_FORMAT(t.created,'%Y-%m-%d %H:%i') AS fecha_creacion,
      DATE_FORMAT(t.closed,'%Y-%m-%d %H:%i')  AS fecha_cierre,
      st.name       AS estado,
      CONCAT(IFNULL(sf.firstname,''),' ',IFNULL(sf.lastname,'')) AS asignado_a,
      od.name       AS departamento,
      u.name 		AS cliente
    FROM ost_ticket t
    LEFT JOIN ost_ticket_status st ON st.id = t.status_id
    LEFT JOIN ost_staff sf ON sf.staff_id = t.staff_id
    LEFT JOIN ost_department od ON od.id  = t.dept_id
    LEFT JOIN ost_user u ON u.id = t.user_id
    WHERE t.closed IS NOT NULL
    ORDER BY t.closed DESC]]>
	</queryString>
